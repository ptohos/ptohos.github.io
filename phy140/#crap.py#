#!/usr/bin/python3

mytuple = ('abc', 786, 2.32, 'john', 35.)
atuple = (123, 'john')

print(mytuple)
print(mytuple0])
print(mytuple[1:3])
print(mytuple[2:])
print(atuple*2)
print(mytuple+atuple)
newtuple = mytuple + atuple
print(newtuple)



#!/usr/bin/python3

mytuple = ('abcd', 786, 2.32, 'john', 35.)
list = ['abcd',  786, 2.32, 'john', 35.]

mytuple[2] = 1000

list[2] = 1000